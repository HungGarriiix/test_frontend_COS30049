{
    "version": 2,
    "builds": [
      {
        "src": "builds/**",
        "use": "@vercel/static"
      },
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*" 
        }
      }
    ]
  }